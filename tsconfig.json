{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["src/*"],
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@store/*": ["store/*"],
      "@navigation/*": ["navigation/*"],
      "@hooks/*":["hooks/*"],
      "@api/*":["api/*"],
      "@services/*":["services/*"],
    }
  }
}
